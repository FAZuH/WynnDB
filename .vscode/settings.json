{
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingTypeStubs": "none",
    "reportUnknownArgumentType": "none",
    "reportUnknownMemberType": "none",
    "reportUnknownVariableType": "none"
  },
  "editor.bracketPairColorization.enabled": false,
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./tests",
    "-p",
    "test_*.py"
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.unittestEnabled": true,
  "highlight.decorations": {
    "rangeBehavior": 3,
  },
  "highlight.regexFlags": "g",
  "highlight.regexes": {
    "\\b(ConnectionPool|Table|GuildHistoryBase|GuildInfoBase|GuildMemberHistoryBase|CharacterHistoryBase|CharacterInfoBase|OnlinePlayersBase|PlayerActivityHistoryBase|PlayerHistoryBase|PlayerInfoBase|CharacterHistoryId|CharacterInfoId|GuildHistoryId|GuildInfoId|GuildMemberHistoryId|OnlinePlayersId|PlayerActivityHistoryId|PlayerHistoryId|PlayerInfoId)\\b": {
      "backgroundColor": "#436850", "color": "#FBFADA"
    },
    "\\b(Fetch|Request)\\b": {
      "backgroundColor": "#3F72AF", "color": "black", "fontWeight": "bold"
    }
  }
}